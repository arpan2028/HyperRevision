class Solution(object):
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        n = len(nums)
        maxInd = 0

        for i in range(n):
            if i > maxInd:
                return False
            maxInd = max(maxInd, i + nums[i])
        return True
#TC - O(n)
#SC - O(1)        

