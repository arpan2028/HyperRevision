class Solution(object):
    def jump(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        n = len(nums)
        farthest = 0
        jumps = 0
        currentEnd =  0

        for i in range(n - 1):
            farthest = max(farthest, i + nums[i])
            if i == currentEnd:
                jumps += 1
                currentEnd = farthest
        return jumps

# TC- O(n)
# SC - O(1)
