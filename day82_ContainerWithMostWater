class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        maxarea = 0
        l, r = 0, len(height) - 1
        while l < r:
            currarea = min(height[r], height[l]) * (r - l)
            maxarea = max(maxarea, currarea)
            if height[l] < height[r]:
                l += 1
            else:
                r -= 1
        return maxarea

# TC - O(n)
# SC - O(1)
